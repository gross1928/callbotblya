{
  "name": "daeeda-food-bot",
  "version": "1.0.0",
  "description": "Telegram bot for food tracking, calories, water and medical data",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "type-check": "tsc --noEmit",
    "import-products": "ts-node scripts/import-products.ts",
    "import-json": "ts-node scripts/import-from-json.ts"
  },
  "keywords": [
    "telegram",
    "bot",
    "food",
    "tracking",
    "nutrition"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@sentry/node": "^10.19.0",
    "@sentry/profiling-node": "^10.19.0",
    "@supabase/supabase-js": "^2.38.4",
    "axios": "^1.6.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "openai": "^4.20.1",
    "pg": "^8.11.3",
    "sharp": "^0.32.6",
    "telegraf": "^4.15.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/pg": "^8.10.9",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.54.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.2"
  }
}
